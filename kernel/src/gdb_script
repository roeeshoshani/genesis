set architecture mips:isa32r2
set endian little 

define hook-stop
x/4i $pc
end

define hook-add-symbol-file
set confirm off
end

define hookpost-add-symbol-file
set confirm on
end

add-symbol-file loader/target/target/release/loader
add-symbol-file core/target/target/debug/core 0x80800300

set language rust
set print pretty on
source $(rustc --print sysroot)/lib/rustlib/etc/gdb_load_rust_pretty_printers.py

target remote localhost:1234

